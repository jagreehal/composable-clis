{
	"name": "composable-command-line-interfaces",
	"version": "1.0.0",
	"main": "./bin/index.js",
	"private": true,
	"license": "MIT",
	"scripts": {
		"dev": "tsx src/index.ts",
		"build": "tsup src/index.ts --format cjs --dts --outDir bin",
