	},
	"dependencies": {
		"commander": "^11.1.0"
	},
	"devDependencies": {
		"@types/node": "^20.8.10",
		"eslint-config-jag-nodejs": "^1.0.0",
		"prettier": "^3.0.3",
		"tsup": "^7.2.0",
		"tsx": "^3.14.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	}
}
